<form method="post" action="{{ path('etudient_delete', {'id': etudient.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ etudient.id) }}">
   <br><br>
    <button class="genric-btn danger">Delete</button>
</form>
